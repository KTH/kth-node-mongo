  },
  "license": "MIT",
  "dependencies": {
    "@kth/log": "^4.0.6"
  },
  "peerdependencies": {
    "mongoose": "^7.0.0"
  },
  "overrides": {
    "semver-regex": "^3.1.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@kth/eslint-config-kth": "^3.0.13",
    "@types/jest": "^29.4.0",
    "ansi-regex": "^6.0.1",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "jest-extended": "^3.2.4",
    "mongoose": "^7.4.2",
    "node-notifier": "^10.0.1",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "set-value": "^4.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "prettier": {
